{
  "expo": {
    "name": "TodOk List",
    "slug": "todok",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "I really loved building this app.\nA simple Todo List was created to list daily tasks using React Native and exploring some properties and mobile elements, the data is saved in the Storage cache.",
    "githubUrl": "https://github.com/paulpessoa/TodOk"
  }
}
